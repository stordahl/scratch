{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "scratch",
  "title": "Scratch",
  "description": "Your digital scratch paper",
  "icon": "command-icon.png",
  "author": "stordahl",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "scratch",
      "title": "scratch",
      "subtitle": "Open your scratch pad",
      "description": "Opens your scratch pad",
      "mode": "view"
    },
    {
      "name": "scratch-clear",
      "title": "scratch-clear",
      "subtitle": "Clear your scratch",
      "description": "Clears your scratch pad",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}